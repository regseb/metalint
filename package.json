{
    "name": "metalint",
    "version": "0.8.8",
    "description": "Un linter pour les gouverner tous.",
    "keywords": [
        "checker",
        "lint",
        "metalint",
        "quality",
        "static-analysis"
    ],
    "homepage": "https://regseb.github.io/metalint/",
    "bugs": {
        "url": "https://github.com/regseb/metalint/issues"
    },
    "license": "EUPL-1.2",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "files": [
        "bin/",
        "lib/",
        "help/"
    ],
    "main": "lib/index",
    "bin": "bin/cli.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/regseb/metalint.git"
    },
    "funding": "https://www.paypal.me/sebastienregne",
    "scripts": {
        "lint": "bin/cli.js",
        "test": "c8 mocha test/lib/",
        "coverage:html": "c8 report --reporter=html",
        "coverage:lcov": "c8 report --reporter=lcovonly",
        "jsdocs": "jsdoc -r -d jsdocs/ lib/",
        "release": "standard-version",
        "clean": "rm -rf coverage/ jsdocs/ node_modules/"
    },
    "dependencies": {
        "colors": "^1.4.0",
        "jsonlint-mod": "^1.7.5",
        "npmlog": "^4.1.2",
        "yargs": "^15.3.1"
    },
    "devDependencies": {
        "addons-linter": "^1.24.0",
        "c8": "^7.1.2",
        "coffeelint": "^2.1.0",
        "csslint": "^1.0.5",
        "david": "^12.0.0",
        "doiuse": "^4.2.0",
        "eslint": "^7.0.0",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsdoc": "^25.4.1",
        "eslint-plugin-mocha": "^7.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-unicorn": "^20.0.0",
        "flow-bin": "^0.125.0",
        "htmlhint": "^0.12.2",
        "htmllint": "^0.8.0",
        "jscs": "^3.0.7",
        "jsdoc": "^3.6.4",
        "jshint": "^2.11.1",
        "json-lint": "^0.1.0",
        "jsonlint": "^1.6.3",
        "lesshint": "^6.3.7",
        "markdownlint": "^0.20.2",
        "memory-streams": "^0.1.3",
        "mocha": "^7.1.2",
        "purgecss": "^2.2.1",
        "standard": "^14.3.4",
        "standard-version": "^8.0.0",
        "stylelint": "^13.3.3",
        "tslint": "^6.1.2",
        "typescript": "^3.9.2",
        "yaml-lint": "^1.2.4"
    },
    "engines": {
        "node": ">=12.0.0"
    }
}
