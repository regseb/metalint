{
    "name": "metalint",
    "version": "0.10.0",
    "description": "Un linter pour les gouverner tous.",
    "keywords": [
        "checker",
        "lint",
        "metalint",
        "quality",
        "static-analysis"
    ],
    "homepage": "https://github.com/regseb/metalint#readme",
    "bugs": {
        "url": "https://github.com/regseb/metalint/issues",
        "email": "regseb@gmail.com"
    },
    "license": "MIT",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "funding": "https://www.paypal.me/sebastienregne",
    "files": [
        "src/",
        "types/"
    ],
    "main": "src/core/index.js",
    "bin": "src/bin/index.js",
    "types": "types/index.d.ts",
    "repository": "regseb/metalint",
    "type": "module",
    "scripts": {
        "lint": "src/bin/index.js",
        "lint:types": "tsc --project .tsconfig_lint.json",
        "test": "c8 mocha test/",
        "coverage:html": "c8 report --reporter html",
        "coverage:lcov": "c8 report --reporter lcovonly",
        "jsdocs": "typedoc --tsconfig .tsconfig_jsdocs.json --out jsdocs/ src/core/",
        "prepare": "tsc --project .tsconfig_types.json",
        "release": "standard-version",
        "clean": "node .script/clean.js"
    },
    "dependencies": {
        "chalk": "^5.0.1",
        "yargs": "^17.3.1"
    },
    "devDependencies": {
        "@coffeelint/cli": "5.2.5",
        "@types/eslint": "8.4.1",
        "@linthtml/linthtml": "0.8.6",
        "@types/htmlhint": "1.1.2",
        "@types/jshint": "2.12.1",
        "@types/mocha": "9.1.0",
        "@types/mock-fs": "4.13.1",
        "@types/node": "17.0.23",
        "@types/sinon": "10.0.11",
        "@types/yargs": "17.0.10",
        "addons-linter": "5.0.0",
        "c8": "7.11.0",
        "coffeelint": "2.1.0",
        "csslint": "1.0.5",
        "doiuse": "4.4.1",
        "eslint": "8.12.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.25.4",
        "eslint-plugin-jsdoc": "38.1.4",
        "eslint-plugin-mocha": "10.0.3",
        "eslint-plugin-no-unsanitized": "4.0.1",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-promise": "6.0.0",
        "eslint-plugin-regexp": "1.6.0",
        "eslint-plugin-unicorn": "41.0.1",
        "flow-bin": "0.174.1",
        "htmlhint": "1.1.3",
        "htmllint": "0.8.0",
        "jshint": "2.13.4",
        "json-lint": "0.1.0",
        "jsonlint": "1.6.3",
        "jsonlint-mod": "1.7.6",
        "lesshint": "6.3.7",
        "markdownlint": "0.25.1",
        "mocha": "9.2.2",
        "mock-fs": "5.1.2",
        "purgecss": "4.1.3",
        "sinon": "13.0.1",
        "standard": "16.0.4",
        "standard-version": "9.3.2",
        "stylelint": "14.6.1",
        "typedoc": "0.22.13",
        "typescript": "4.6.3",
        "yaml-lint": "1.2.4"
        "npm-package-json-lint": "6.3.0",
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
