{
    "name": "metalint",
    "version": "0.8.9",
    "description": "Un linter pour les gouverner tous.",
    "keywords": [
        "checker",
        "lint",
        "metalint",
        "quality",
        "static-analysis"
    ],
    "homepage": "https://regseb.github.io/metalint/",
    "bugs": {
        "url": "https://github.com/regseb/metalint/issues"
    },
    "license": "MIT",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "files": [
        "bin/",
        "lib/",
        "help/"
    ],
    "main": "lib/index",
    "bin": "bin/index.js",
    "repository": "regseb/metalint",
    "funding": "https://www.paypal.me/sebastienregne",
    "type": "module",
    "scripts": {
        "lint": "bin/index.js",
        "tsc:lint": "tsc",
        "test": "c8 mocha test/lib/",
        "coverage:html": "c8 report --reporter=html",
        "coverage:lcov": "c8 report --reporter=lcovonly",
        "jsdocs": "jsdoc -r -d jsdocs/ lib/",
        "release": "standard-version",
        "clean": "rm -rf coverage/ jsdocs/ node_modules/"
    },
    "dependencies": {
        "chalk": "^4.1.0",
        "jsonlint-mod": "^1.7.6",
        "npmlog": "^4.1.2",
        "yargs": "^16.1.1"
    },
    "devDependencies": {
        "@types/eslint": "^7.2.5",
        "@types/mocha": "^8.0.4",
        "@types/node": "^14.14.9",
        "@types/sinon": "^9.0.8",
        "@types/stylelint": "^9.10.1",
        "@types/yargs": "^15.0.10",
        "addons-linter": "^2.12.0",
        "c8": "^7.3.5",
        "coffeelint": "^2.1.0",
        "csslint": "^1.0.5",
        "david": "^12.0.0",
        "doiuse": "^4.4.0",
        "eslint": "^7.13.0",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsdoc": "^30.7.8",
        "eslint-plugin-mocha": "^8.0.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-unicorn": "^23.0.0",
        "eslint-import-resolver-node": "^0.3.4",
        "flow-bin": "^0.138.0",
        "htmlhint": "^0.14.2",
        "htmllint": "^0.8.0",
        "jsdoc": "^3.6.6",
        "jshint": "^2.12.0",
        "json-lint": "^0.1.0",
        "jsonlint": "^1.6.3",
        "lesshint": "^6.3.7",
        "markdownlint": "^0.21.1",
        "memory-streams": "^0.1.3",
        "mocha": "^8.2.1",
        "purgecss": "^3.0.0",
        "sinon": "^9.2.1",
        "standard": "^16.0.3",
        "standard-version": "^9.0.0",
        "stylelint": "^13.8.0",
        "tslint": "^6.1.3",
        "typescript": "^4.1.2",
        "yaml-lint": "^1.2.4"
    },
    "engines": {
        "node": ">=14.0.0"
    }
}
