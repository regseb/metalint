{
    "name": "metalint",
    "version": "0.11.0",
    "description": "Un linter pour les gouverner tous.",
    "keywords": [
        "checker",
        "lint",
        "metalint",
        "quality",
        "static-analysis"
    ],
    "homepage": "https://github.com/regseb/metalint#readme",
    "bugs": {
        "url": "https://github.com/regseb/metalint/issues",
        "email": "regseb@gmail.com"
    },
    "license": "MIT",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "funding": "https://www.paypal.me/sebastienregne",
    "files": [
        "src/",
        "types/"
    ],
    "main": "src/core/index.js",
    "types": "types/index.d.ts",
    "bin": "src/bin/index.js",
    "repository": "regseb/metalint",
    "type": "module",
    "scripts": {
        "lint": "src/bin/index.js",
        "lint:types": "tsc --project .tsconfig_lint.json",
        "test": "c8 mocha --config test/mocharc.json",
        "coverage:html": "c8 report --reporter html",
        "coverage:lcov": "c8 report --reporter lcovonly",
        "jsdocs": "typedoc --tsconfig .tsconfig_jsdocs.json --out jsdocs/ src/core/",
        "prepare": "tsc --project .tsconfig_types.json",
        "release": "standard-version",
        "clean": "node .script/clean.js"
    },
    "dependencies": {
        "chalk": "5.1.2",
        "yargs": "17.6.2"
    },
    "devDependencies": {
        "@coffeelint/cli": "5.2.11",
        "@linthtml/linthtml": "0.9.3",
        "@types/debug": "4.1.7",
        "@types/eslint": "8.4.10",
        "@types/file-entry-cache": "5.0.2",
        "@types/htmlhint": "1.1.2",
        "@types/js-yaml": "4.0.5",
        "@types/jshint": "2.12.1",
        "@types/mocha": "10.0.0",
        "@types/mock-fs": "4.13.1",
        "@types/node": "18.11.9",
        "@types/sinon": "10.0.13",
        "@types/yargs": "17.0.13",
        "addons-linter": "5.21.0",
        "c8": "7.12.0",
        "csslint": "1.0.5",
        "doiuse": "4.4.1",
        "eslint": "8.27.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsdoc": "39.6.2",
        "eslint-plugin-mocha": "10.1.0",
        "eslint-plugin-n": "15.5.1",
        "eslint-plugin-no-unsanitized": "4.0.1",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-regexp": "1.10.0",
        "eslint-plugin-unicorn": "44.0.2",
        "flow-bin": "0.193.0",
        "htmlhint": "1.1.4",
        "htmllint": "0.8.0",
        "jshint": "2.13.6",
        "json-lint": "0.1.0",
        "jsonlint": "1.6.3",
        "jsonlint-mod": "1.7.6",
        "lesshint": "6.3.7",
        "markdownlint": "0.26.2",
        "markuplint": "2.11.0",
        "mocha": "10.1.0",
        "mock-fs": "5.2.0",
        "npm-package-json-lint": "6.3.0",
        "purgecss": "5.0.0",
        "sinon": "14.0.2",
        "standard": "17.0.0",
        "standard-version": "9.5.0",
        "stylelint": "14.15.0",
        "typedoc": "0.23.21",
        "typescript": "4.8.4",
        "yaml-lint": "1.7.0"
    },
    "engines": {
        "node": ">=18.12.0"
    }
}
