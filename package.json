{
  "name": "metalint",
  "version": "0.11.0",
  "description": "Un linter pour les gouverner tous.",
  "keywords": [
    "checker",
    "lint",
    "metalint",
    "quality",
    "static-analysis"
  ],
  "homepage": "https://github.com/regseb/metalint#readme",
  "bugs": {
    "url": "https://github.com/regseb/metalint/issues",
    "email": "regseb@gmail.com"
  },
  "license": "MIT",
  "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
  "funding": "https://www.paypal.me/sebastienregne",
  "files": [
    "src/",
    "types/"
  ],
  "main": "src/core/index.js",
  "types": "types/index.d.ts",
  "bin": "src/bin/index.js",
  "repository": "regseb/metalint",
  "type": "module",
  "scripts": {
    "lint": "src/bin/index.js",
    "lint:fix": "src/bin/index.js --fix",
    "lint:types": "tsc --project .tsconfig_lint.json",
    "test": "npm run test:unit",
    "test:unit": "mocha --config test/mocharc.json",
    "test:coverage": "stryker run .stryker.config.js",
    "jsdocs": "typedoc --tsconfig .tsconfig_jsdocs.json --out jsdocs/ src/core/",
    "prepare": "tsc --project .tsconfig_types.json",
    "release": "standard-version",
    "clean": "node .script/clean.js"
  },
  "dependencies": {
    "chalk": "5.2.0",
    "yargs": "17.7.1"
  },
  "devDependencies": {
    "@coffeelint/cli": "5.2.11",
    "@prettier/plugin-xml": "2.2.0",
    "@stryker-mutator/core": "6.4.1",
    "@stryker-mutator/mocha-runner": "6.4.1",
    "@types/debug": "4.1.7",
    "@types/eslint": "8.21.1",
    "@types/file-entry-cache": "5.0.2",
    "@types/htmlhint": "1.1.2",
    "@types/js-yaml": "4.0.5",
    "@types/jshint": "2.12.1",
    "@types/mocha": "10.0.1",
    "@types/mock-fs": "4.13.1",
    "@types/node": "18.14.6",
    "@types/sinon": "10.0.13",
    "@types/yargs": "17.0.22",
    "addons-linter": "5.30.0",
    "csslint": "1.0.5",
    "doiuse": "4.4.1",
    "eslint": "8.35.0",
    "eslint-plugin-array-func": "3.1.8",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsdoc": "40.0.1",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-no-unsanitized": "4.0.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "1.12.0",
    "eslint-plugin-unicorn": "46.0.0",
    "flow-bin": "0.201.0",
    "htmlhint": "1.1.4",
    "htmllint": "0.8.0",
    "jshint": "2.13.6",
    "json-lint": "0.1.0",
    "jsonlint": "1.6.3",
    "jsonlint-mod": "1.7.6",
    "lesshint": "6.3.7",
    "markdownlint": "0.27.0",
    "markuplint": "3.3.0",
    "mocha": "10.2.0",
    "mock-fs": "5.2.0",
    "npm-package-json-lint": "6.4.0",
    "prettier": "2.8.4",
    "purgecss": "5.0.0",
    "sinon": "15.0.1",
    "standard": "17.0.0",
    "standard-version": "9.5.0",
    "stylelint": "15.2.0",
    "typedoc": "0.23.26",
    "typescript": "4.9.5",
    "yaml-lint": "1.7.0"
  },
  "engines": {
    "node": ">=18.12.0"
  }
}
