{
    "name": "metalint",
    "version": "0.8.3",
    "description": "Un linter pour les gouverner tous.",
    "keywords": [
        "checker",
        "lint",
        "metalint",
        "quality",
        "static-analysis"
    ],
    "homepage": "https://regseb.github.io/metalint/",
    "bugs": {
        "url": "https://github.com/regseb/metalint/issues"
    },
    "license": "EUPL-1.2",
    "author": "Sébastien Règne <regseb@gmail.com> (https://github.com/regseb)",
    "files": [
        "bin/",
        "lib/",
        "help/"
    ],
    "main": "lib/index",
    "bin": "bin/cli.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/regseb/metalint.git"
    },
    "scripts": {
        "lint": "bin/cli.js",
        "test": "nyc mocha test/lib/",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "coverage:html": "nyc report --reporter=html",
        "jsdocs": "jsdoc -r -d jsdocs/ lib/",
        "release": "standard-version",
        "clean": "rm -rf .nyc_output/ coverage/ jsdocs/ node_modules/"
    },
    "dependencies": {
        "colors": "^1.3.3",
        "jsonlint": "^1.6.3",
        "npmlog": "^4.1.2",
        "yargs": "^14.0.0"
    },
    "devDependencies": {
        "addons-linter": "^1.11.1",
        "coffeelint": "^2.1.0",
        "coveralls": "^3.0.6",
        "csslint": "^1.0.5",
        "david": "^11.0.0",
        "doiuse": "^4.2.0",
        "eslint": "6.1.0",
        "eslint-plugin-filenames": "^1.3.2",
        "eslint-plugin-jsdoc": "^15.8.0",
        "eslint-plugin-mocha": "^6.0.0",
        "eslint-plugin-promise": "^4.2.1",
        "flow-bin": "^0.105.0",
        "htmlhint": "^0.11.0",
        "htmllint": "^0.8.0",
        "jscs": "^3.0.7",
        "jsdoc": "^3.6.3",
        "jshint": "^2.10.2",
        "json-lint": "^0.1.0",
        "lesshint": "^6.3.7",
        "markdownlint": "^0.16.0",
        "memory-streams": "^0.1.3",
        "mocha": "^6.2.0",
        "nyc": "^14.1.1",
        "purgecss": "^1.3.0",
        "standard-version": "^7.0.0",
        "standard": "^13.1.0",
        "stylelint": "^10.1.0",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3",
        "yaml-lint": "^1.2.4"
    }
}
